message <span class="message">GetRepeatedDataArg</span>
{
  <span class="comment">/** 
   * How many integers to send back.
   */</span>
  required <span class="number">uint32</span> integers = 1; 
  <span class="comment">/** 
   * How many booleans to send back.
   */</span>
  required <span class="number">uint32</span> booleans = 2; 
  <span class="comment">/** 
   * How many strings to send back.
   */</span>
  required <span class="number">uint32</span> strings  = 3; 
  <span class="comment">/** 
   * How many Nested messages to send back. Each Nested
   * also has a list of MoreNested messages. In each of
   * the 'n' Nested messages, there will be 'n' MoreNested 
   * messages.
   */</span>
  required <span class="number">uint32</span> nested   = 4; 
}
